{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "Cargo.lock": true,
    "target": true,
  },
  "rust-analyzer.showUnlinkedFileNotification": false,
  "rust-analyzer.cargo.extraArgs": [
    "--target-dir",
    "${workspaceFolder}/target/check"
  ],
  "cSpell.words": [
    "linebreak"
  ],
}
